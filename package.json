{
  "name": "ember-yup",
  "version": "1.0.4",
  "description": "Dead, simple object schema validations for Ember",
  "keywords": [
    "ember-addon",
    "validate",
    "validation",
    "form",
    "forms",
    "schema"
  ],
  "homepage": "https://egaba.github.io/ember-yup/",
  "repository": {
    "type": "git",
    "url": "https://github.com/egaba/ember-yup.git"
  },
  "license": "MIT",
  "author": "Edward Gaba <egaba88@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "deploy": "rm -rf docs && ember build -prod --output-path docs && git add . && git commit -am 'deploy gh page' && git push || true",
    "postversion": "npm run deploy && npm publish && git push --follow-tags"
  },
  "peerDependencies": {
    "yup": "0.x"
  },
  "dependencies": {
    "ember-cli-babel": "^6.16.0",
    "ember-cli-cjs-transform": "^1.3.1",
    "ember-cli-htmlbars": "^3.0.0",
    "yup": "0.27.x"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.6.3",
    "broccoli-asset-rev": "^2.7.0",
    "doctrine": "^3.0.0",
    "ember-ajax": "^3.1.0",
    "ember-cli": "~3.5.1",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-showdown": "^4.4.4",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-tailwind": "^0.7.0",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-data": "^3.9.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-faker": "^1.5.0",
    "ember-fetch": "^6.5.1",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-side-menu": "^0.1.0",
    "ember-source": "~3.5.1",
    "ember-source-channel-url": "^1.1.0",
    "ember-truth-helpers": "^2.1.0",
    "ember-try": "^1.0.0",
    "ember-wired": "^1.0.7",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-node": "^8.0.1",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.0",
    "sass": "^1.20.1",
    "wired-elements": "^1.0.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://egaba.github.io/ember-yup/"
  }
}
